        function _ProductsComponent(productsService, router) {
          _classCallCheck(this, _ProductsComponent);

          this.productsService = productsService;
          this.router = router;
          this.products = [];
          this.filteredProducts = [];
          this.loading = true;
          this.error = null;
          this.searchQuery = '';
          this.statusFilter = '';
          this.baseUrl = 'https://2872714c-427f-45d7-86a5-48cfb2ec630d-00-1poko749ejplg.janeway.replit.dev';
          this.uploads = '/uploads';
        }

        return _createClass(_ProductsComponent, [{
          key: "ngOnInit",
          value: function ngOnInit() {
            this.loadProducts();
          }
        }, {
          key: "applyFilters",
          value: function applyFilters() {
            var _this0 = this;

            var query = this.searchQuery.toLowerCase();
            this.filteredProducts = this.products.filter(function (product) {
              // Filtre recherche texte sur le nom
              var matchesSearch = product.name.toLowerCase().includes(query); // Filtre stock selon la sélection

              var matchesStatus = true;

              if (_this0.statusFilter === 'disponible') {
                matchesStatus = product.qtt_stock > 0;
              } else if (_this0.statusFilter === 'epuise') {
                matchesStatus = product.qtt_stock === 0;
              }

              return matchesSearch && matchesStatus;
            });
          }
        }, {
          key: "loadProducts",
          value: function loadProducts() {
            var _this1 = this;

            this.loading = true;
            this.error = null;
            this.productsService.getProducts().subscribe({
              next: function next(data) {
                _this1.products = data;
                _this1.filteredProducts = _toConsumableArray(_this1.products);
                _this1.loading = false;
              },
              error: function error(err) {
                _this1.error = err.message || 'Erreur lors du chargement des produits';
                _this1.loading = false;
              }
            });
          }
        }, {
          key: "getFullImageUrl",
          value: function getFullImageUrl(imageFilename) {
            var imagePath = "".concat(this.baseUrl).concat(this.uploads, "/");

            if (imageFilename && imageFilename.trim() !== '') {
              return "".concat(imagePath).concat(imageFilename);
            }

            return "".concat(imagePath, "no_picture.jpg");
          }
        }, {
          key: "editProduct",
          value: function editProduct(product) {
            this.router.navigate(['/produits/edit/' + product.id]);
          }
        }, {
          key: "deleteProduct",
          value: function deleteProduct(product) {
            var _this10 = this;

            if (confirm('Êtes-vous sûr de vouloir supprimer ce produit ?')) {
              this.productsService.deleteProduct(product.id).subscribe({
                next: function next() {
                  _this10.loadProducts();
                },
                error: function error(err) {
                  alert('Erreur lors de la suppression du produit');
                }
              });
            }
          }
        }]);
