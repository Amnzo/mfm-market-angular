        }, {
          key: "loadDeliveryUsers",
          value: function loadDeliveryUsers() {
            var _this = this;

            this.http.get('/admin/users').subscribe(function (response) {
              console.log(response);
              _this.deliveryUsers = response.filter(function (user) {
                return user.user_level === 'livreur';
              }); // Filtrer par level = 1 pour les livreurs
            });
          }
        }, {
          key: "assignDelivery",
          value: function assignDelivery() {
            var _this2 = this;

            if (this.selectedCommande && this.selectedDeliveryUser) {
              var deliveryData = {
                order_id: this.selectedCommande.id,
                delivery_id: this.selectedDeliveryUser
              };
              this.http.post('/admin/assign-delivery', deliveryData).subscribe(function (response) {
                // Mettre à jour le statut de la commande
                var modal = document.getElementById('assignDeliveryModal');

                if (modal) {
                  modal.classList.remove('show');
                  modal.style.display = 'none';
                }

                _this2.selectedCommande = null;
                _this2.selectedDeliveryUser = null;

                _this2.loadCommandes();
              });
            }
          }
        }, {
          key: "loadCommandes",
          value: function loadCommandes() {
            var _this3 = this;

            this.http.get('/admin/orders').subscribe(function (response) {
              _this3.commandes = response.orders;
            });
          }
        }, {
          key: "filteredCommandes",
          get: function get() {
            var _this4 = this;

            var filtered = _toConsumableArray(this.commandes); // Filtrer par recherche


            if (this.searchTerm) {
              var term = this.searchTerm.toLowerCase();
              filtered = filtered.filter(function (cmd) {
                return cmd.id.toString().includes(term) || cmd.client_name.toLowerCase().includes(term);
              });
            } // Filtrer par statut


            if (this.statusFilter) {
              filtered = filtered.filter(function (cmd) {
                return cmd.status === _this4.statusFilter;
              });
            } // Filtrer par date


            if (this.dateFilter) {
              var filterDate = new Date(this.dateFilter);
              filtered = filtered.filter(function (cmd) {
                var cmdDate = new Date(cmd.created_at);
                return cmdDate.toDateString() === filterDate.toDateString();
              });
            } // Pagination


            var start = (this.currentPage - 1) * this.itemsPerPage;
            return filtered.slice(start, start + this.itemsPerPage);
          }
        }, {
          key: "totalPages",
          get: function get() {
            return Math.ceil(this.commandes.length / this.itemsPerPage);
          }
        }, {
          key: "formatDateTime",
          value: function formatDateTime(date) {
            return new Date(date).toLocaleString();
          }
        }, {
          key: "getStatusColor",
          value: function getStatusColor(status) {
            var statusMap = {
              'en attente': 'warning',
              'en cours': 'info',
              'terminée': 'success',
              'annulée': 'danger',
              'livrée': 'success'
            };
            return statusMap[status.toLowerCase()] || 'default';
          }
        }, {
          key: "imprimerCommande",
          value: function imprimerCommande(commande) {
            var itemsHTML = '';
            commande.items.forEach(function (item) {
              itemsHTML += "\n        <tr>\n          <td>".concat(item.product_name, "</td>\n          <td>").concat(item.quantity, "</td>\n          <td>").concat(item.price, " MAD</td>\n          <td>").concat(item.remise, " %</td>\n          <td>").concat(item.total_ligne, " MAD</td>\n        </tr>\n      ");
            });
            var paiementsHTML = '';
            commande.paiements.forEach(function (paiement) {
              paiementsHTML += "\n        <tr>\n          <td>".concat(new Date(paiement.date_paiement).toLocaleDateString(), "</td>\n          <td>").concat(paiement.montant, " MAD</td>\n          <td>").concat(paiement.mode_paiement, "</td>\n        </tr>\n      ");
            });
            var printContent = "\n      <html>\n      <head>\n        <title>Impression commande #".concat(commande.id, "</title>\n        <link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css\" rel=\"stylesheet\" />\n        <style>\n          body { padding: 20px; font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif; }\n          h2, h3 { text-align: center; margin-bottom: 20px; }\n          .table thead th { background-color: #0d6efd; color: white; }\n          .info-section p { font-size: 1rem; margin: 0.3rem 0; }\n        </style>\n      </head>\n      <body>\n        <div class=\"container\">\n          <h2 class=\"mb-4\">D\xE9tails de la commande #").concat(commande.id, "</h2>\n  \n          <div class=\"info-section mb-4\">\n            <p><strong>Client :</strong> ").concat(commande.client_name, "</p>\n            <p><strong>Adresse :</strong> ").concat(commande.client_adresse, "</p>\n            <p><strong>Mobile :</strong> ").concat(commande.client_mobile, "</p>\n            <p><strong>Status :</strong> <span class=\"badge bg-success\">").concat(commande.status, "</span></p>\n            <p><strong>Date cr\xE9ation :</strong> ").concat(new Date(commande.created_at).toLocaleString(), "</p>\n            <p><strong>Total :</strong> <span class=\"fw-bold\">").concat(commande.total, " MAD</span></p>\n            \n          </div>\n  \n          <h3>Produits</h3>\n          <table class=\"table table-striped table-bordered\">\n            <thead>\n              <tr>\n                <th>Produit</th>\n                <th>Quantit\xE9</th>\n                <th>Prix unitaire</th>\n                <th>Remise (%)</th>\n                <th>Total ligne</th>\n              </tr>\n            </thead>\n            <tbody>\n              ").concat(itemsHTML, "\n            </tbody>\n          </table>\n  \n          <h3>Paiements</h3>\n          <table class=\"table table-bordered\">\n            <thead>\n              <tr>\n                <th>Date</th>\n                <th>Montant</th>\n                <th>Mode de paiement</th>\n              </tr>\n            </thead>\n            <tbody>\n              ").concat(paiementsHTML, "\n            </tbody>\n          </table>\n        </div>\n      </body>\n      </html>\n    ");
            var newWindow = window.open('', '_blank', 'width=900,height=700');

            if (newWindow) {
              newWindow.document.write(printContent);
              newWindow.document.close();
              newWindow.focus();
              newWindow.print();
            }
          }
        }]);
      }();

